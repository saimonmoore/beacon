{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://beacon.org/beacon.schema.json",
  "type": "object",
  "default": {},
  "title": "Beacon Beacon Schema",
  "required": [
    "userId",
    "periodOfTime",
    "activityId",
    "beaconStatus",
    "timestamp",
    "createdAt",
    "updatedAt"
  ],
  "properties": {
    "userId": {
      "type": "string",
      "default": "",
      "title": "The userId Schema",
      "examples": [
        "103283b30db86e1fffd19ffa327d3dc98f079cb6a606f4c5c9c98ee14b405566"
      ]
    },
    "periodOfTime": {
      "type": "number",
      "default": "",
      "title": "The periodOfTime Schema",
      "examples": [
        6
      ]
    },
    "beaconStatus": {
      "type": "string",
      "default": "",
      "title": "The beaconStatus Schema",
      "examples": [
        "alarm"
      ]
    },
    "timestamp": {
      "type": "string",
      "default": "",
      "title": "The timestamp Schema",
      "examples": [
        "2023-03-26T07:09:37.765Z"
      ]
    },
    "createdAt": {
      "type": "string",
      "default": "",
      "title": "The createdAt Schema",
      "examples": [
        "2023-03-26T07:09:37.765Z"
      ]
    },
    "updatedAt": {
      "type": "string",
      "default": "",
      "title": "The updatedAt Schema",
      "examples": [
        "2023-03-26T07:09:37.765Z"
      ]
    }
  },
  "examples": [
    {
      "userId": "103283b30db86e1fffd19ffa327d3dc98f079cb6a606f4c5c9c98ee14b405566",
      "periodOfTime": 6,
      "beaconStatus": "alarm",
      "timestamp": "2023-03-26T07:09:37.765Z",
      "createdAt": "2023-03-26T07:09:37.765Z",
      "updatedAt": "2023-03-26T07:09:37.765Z"
    }
  ]
}
