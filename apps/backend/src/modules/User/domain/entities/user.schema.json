{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://beacon.org/user.schema.json",
  "type": "object",
  "default": {},
  "title": "Beacon User Schema",
  "required": [
    "hash",
    "email",
    "password",
    "firstName",
    "lastName",
    "dateOfBirth",
    "sex",
    "profilePictureUrl",
    "createdAt",
    "updatedAt"
  ],
  "properties": {
    "hash": {
      "type": "string",
      "default": "",
      "title": "The hash Schema",
      "examples": [
        "103283b30db86e1fffd19ffa327d3dc98f079cb6a606f4c5c9c98ee14b405566"
      ]
    },
    "firstName": {
      "type": "string",
      "default": "",
      "title": "The firstName Schema",
      "examples": [
        "Simon"
      ]
    },
    "lastName": {
      "type": "string",
      "default": "",
      "title": "The lastName Schema",
      "examples": [
        "Moore"
      ]
    },
    "profilePictureUrl": {
      "type": "string",
      "default": "",
      "title": "The profilePictureUrl Schema",
      "examples": [
        "https://upload.wikimedia.org/wikipedia/commons/b/b6/Simon_Moore_2014.jpg"
      ]
    },
    "email": {
      "type": "string",
      "default": "",
      "title": "The email Schema",
      "examples": [
        "simon.moore@beacon.org"
      ]
    },
    "password": {
      "type": "string",
      "default": "",
      "title": "The password Schema",
      "examples": [
        "secret"
      ]
    },
    "sex": {
      "type": "string",
      "default": "",
      "title": "The sex Schema",
      "examples": [
        "m"
      ]
    },
    "dateOfBirth": {
      "type": "string",
      "default": "",
      "title": "The dateOfBirth Schema",
      "examples": [
        "2023-03-26"
      ]
    },
    "createdAt": {
      "type": "string",
      "default": "",
      "title": "The createdAt Schema",
      "examples": [
        "2023-03-26T07:09:37.765Z"
      ]
    },
    "updatedAt": {
      "type": "string",
      "default": "",
      "title": "The updatedAt Schema",
      "examples": [
        "2023-03-26T07:09:37.765Z"
      ]
    }
  },
  "examples": [
    {
      "hash": "103283b30db86e1fffd19ffa327d3dc98f079cb6a606f4c5c9c98ee14b405566",
      "email": "simon.moore@beacon.org",
      "password": "secret",
      "firstName": "Simon",
      "lastName": "Moore",
      "sex": "m",
      "dateOfBirth": "2023-03-26",
      "profilePictureUrl": "https://upload.wikimedia.org/wikipedia/commons/b/b6/Simon_Moore_2014.jpg",
      "createdAt": "2023-03-26T07:09:37.765Z",
      "updatedAt": "2023-03-26T07:09:37.765Z"
    }
  ]
}
